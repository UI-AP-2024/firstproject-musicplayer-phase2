<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="initialAnchor" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.phase2.ShowAudiosController">
   <children>
      <HBox fx:id="hBox" prefHeight="600.0" prefWidth="800.0">
         <children>
            <VBox fx:id="vBox1" prefHeight="600.0" prefWidth="725.0">
               <children>
                  <AnchorPane fx:id="secondaryAnchor" prefHeight="480.0" prefWidth="725.0">
                     <children>
                        <Label fx:id="audios_lbl" alignment="CENTER" layoutX="297.0" layoutY="7.0" prefHeight="60.0" prefWidth="130.0" text="All Audios">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <VBox fx:id="vBox3" layoutX="56.0" layoutY="79.0" prefHeight="400.0" prefWidth="670.0" />
                     </children>
                  </AnchorPane>
               </children>
            </VBox>
            <VBox fx:id="vBox2" prefHeight="200.0" prefWidth="75.0">
               <children>
                  <Button fx:id="home_btn" mnemonicParsing="false" onAction="#homeAction" prefHeight="60.0" prefWidth="75.0" text="Home" />
                  <Button fx:id="search_btn" mnemonicParsing="false" onAction="#searchAction" prefHeight="60.0" prefWidth="75.0" text="Search" />
                  <Button fx:id="library_btn" mnemonicParsing="false" onAction="#libraryAction" prefHeight="60.0" prefWidth="75.0" text="Library" />
                  <Button fx:id="artists_btn" mnemonicParsing="false" onAction="#artistsAction" prefHeight="60.0" prefWidth="76.0" text="All Artists" />
                  <Button fx:id="audios_btn" mnemonicParsing="false" onAction="#audiosAction" prefHeight="60.0" prefWidth="75.0" text="All Audios" />
                  <Button fx:id="back_btn" mnemonicParsing="false" onAction="#backAction" prefHeight="60.0" prefWidth="75.0" text="Back" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
