<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="anchor" prefHeight="120.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.phase2.Playbar">
   <children>
      <Button fx:id="playOrPause" layoutY="1.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="600.0" text="play" />
      <HBox fx:id="hBox" layoutY="80.0" prefHeight="40.0" prefWidth="600.0">
         <children>
            <Button fx:id="previous" mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" text="previous" />
            <Button fx:id="next" mnemonicParsing="false" prefHeight="40.0" prefWidth="300.0" text="next" />
         </children>
      </HBox>
      <Label fx:id="audioName" alignment="CENTER" layoutY="41.0" prefHeight="40.0" prefWidth="600.0" text="AudioName" />
   </children>
</AnchorPane>
